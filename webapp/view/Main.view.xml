<mvc:View controllerName="geonosis.table.report.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">


    <Page id="page" title="{i18n>title}">
        <content>
            <IconTabBar>
                <items>
                    <IconTabFilter text="Producto" key="Producto">
                        <core:Fragment fragmentName="geonosis.table.report.view.fragment.FilterBar"/>
                    </IconTabFilter>
                    <IconTabFilter text="Detalle" key="Detalle">
                        <core:Fragment fragmentName="geonosis.table.report.view.fragment.Form" type="XML" />
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </content>
    </Page>
</mvc:View>